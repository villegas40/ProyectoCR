@model CasasRed_Nuevo3_.Models.CasaInventario

@{
    ViewBag.Title = "Habilitacion > Detalles asignación";
}

    <div>
        <h4>CasaInventario</h4>
        <hr />
        <table class="table table-bordered">
            <tr>
                <th>Artículo</th>
                <td>@Html.DisplayFor(model => model.ci_articulo_id)</td>
            </tr>
            <tr>
                <th>Cantidad asignada</th>
                <td>@Html.DisplayFor(model => model.ci_cantidadAsignada)</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>@Model.ci_fecha.ToShortDateString()</td>
            </tr>
            <tr>
                <th>Usuario asigno</th>
                <td>@Html.DisplayFor(model => model.Usuario.usu_username)</td>
            </tr>
        </table>
</div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.ci_Id }) | @Html.ActionLink("Regresar", "Index", new { id = Model.ci_corretaje_id }, new { @class= "btn btn-outline-secundary" })
    </p>

