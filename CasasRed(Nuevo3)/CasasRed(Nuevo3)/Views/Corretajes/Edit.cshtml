@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        //Si se tiene fecha y se quiere editar al borrarla completamente convierte el input en un date
        function funcionDate() {
            var x = document.getElementById("fechanac");
            if (x.value == null || x.value == "") {
                x.setAttribute('type', 'date');
            }
        }

        (document.body).setAttribute("onload", "resetCheckbox();");

        function resetCheckbox() {
            var checkBox = document.getElementsByTagName("input");
            var variable = [];
            for (var i = 0; i < checkBox.length; i++) {
                if (checkBox[i].getAttribute("type") == "checkbox") {
                    variable.push(checkBox[i])
                }
            }
            for (var i = 0; i < variable.length; i++) {
                if (variable[i].checked == false) {
                    variable[i].value = "";
                }
            }
        }

        resetCheckbox();

        $(document).ready(function () {
            $(".data").on('change paste', function () {
                var dataCount = $('input.data').length;
                var myFilledDataCount = $('input.data').filter(function () {
                    return $(this).val() === "";
                }).length;
                var width = ((1 / dataCount) * (dataCount - myFilledDataCount)) * 100;
                var mydataPercent = Math.round(width) + "%";
                $(".progress-bar").css("width", mydataPercent);
                $("#bar").text(mydataPercent + " Completado");

                //Guardar porcentaje
                var x = document.getElementById("progreso");
                x.setAttribute('value', Math.round(width));

            });
        });

        // Get the modal
        var modal = document.getElementById('myModal');

        // Get the image and insert it inside the modal - use its "alt" text as a caption
        var img = document.getElementById('imgAgua');
        var img1 = document.getElementById('imgLuz');
        var img2 = document.getElementById('imgPredial');
        var img3 = document.getElementById('imgOtros');
        var modalImg = document.getElementById("img01");
        var captionText = document.getElementById("caption");
        img.onclick = function () {
            modal.style.display = "block";
            modalImg.src = this.src;
            captionText.innerHTML = this.alt;
        }
        img1.onclick = function () {
            modal.style.display = "block";
            modalImg.src = this.src;
            captionText.innerHTML = this.alt;
        }
        img2.onclick = function () {
            modal.style.display = "block";
            modalImg.src = this.src;
            captionText.innerHTML = this.alt;
        }
        img3.onclick = function () {
            modal.style.display = "block";
            modalImg.src = this.src;
            captionText.innerHTML = this.alt;
        }

        // Get the <span> element that closes the modal
        var span = document.getElementsByClassName("close")[0];

        // When the user clicks on <span> (x), close the modal
        span.onclick = function () {
            modal.style.display = "none";
        }

        function sumar() {
            var x = document.getElementById("adeudo_luz");
            var y = document.getElementById("adeudo_agua");
            var yx = document.getElementById("adeudo_predial");
            var z = document.getElementById("gasto");

            if (x.value === "") {
                x.value = 0;
            }
            else if (y.value==="") {
                y.value = 0;
            }
            else if (yx.value === "") {
                yx.value = 0;
            }

            var res = parseFloat(x.value) + parseFloat(y.value) + parseFloat(yx.value);

            z.value = parseFloat(res);
        }

    </script>

}
