@model IEnumerable<CasasRed_Nuevo3_.Models.Corretaje>

@{
    ViewBag.Title = "Index";
}
<br />
<h2>Corretaje Index</h2>
<h4>Lic. Iván Argotte</h4>

<p>
    @Html.ActionLink("+ Nueva Casa Create New", "Create")
</p>

<table class="table table-striped table-responsive-lg">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Dirección</th>
            <th scope="col">Cliente</th>
            <th scope="col">Fecha de Ingreso</th>
            <th scope="col">Status</th>
            <th scope="col">Opciones</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Crt_Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Crt_Cliente_Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Crt_Ano_compra) <!--Cambiar, agregar fecha de alta-->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Crt_Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}
    </tbody>
</table>

